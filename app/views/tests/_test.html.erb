<tr>
  <td><%= test.title %></td>
  <td align="center"><%= test.level %></td>
  <td align="center"><%= test.questions.count %></td>
  <% if test.questions.any? %>
    <td><%= button_to t('start'), start_test_path(test), class: 'btn btn-primary btn-sm' %></td>
  <% else %>
    <td class="text-muted"><%= t('no_questions') %></td>
  <% end %>
</tr>
